{
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "#src/*": ["./src/*"],
      "#app/*": ["./src/scenes/App/*"]
    },
    "jsx": "react"
  },
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Recommended"
}